{
	"manifest_version": 2,

	"name": "The Wordinary",
	"description": "",
	"version": "1.0",
   "background": {
    "scripts": ["background.js","jquery.min.js"]
  },
	"permissions": [
    "notifications",
     "contextMenus"
	],
     "icons": {
        "128": "thewordinary.png",
        "16" : "thewordinary.png",
        "48" : "thewordinary.png"  
    },
    "browser_action": {
    "default_icon": "thewordinary.png",
    "default_popup": "popup.html"
  }
 
}